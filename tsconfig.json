{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": true,
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "baseUrl": "."
  },
  "include": ["src"]
}
